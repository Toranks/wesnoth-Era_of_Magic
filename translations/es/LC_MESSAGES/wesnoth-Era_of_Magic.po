#: Era_of_Magic/units/Tharis/Blade_Dancer.cfg:62
#: Era_of_Magic/units/Tharis/Crimson_Blade.cfg:74
#: Era_of_Magic/units/Tharis/Frontliner.cfg:62
#: Era_of_Magic/units/Tharis/Sword_Dancer.cfg:49
msgid "chakram"
msgstr "chakram"

#. [unit_type]: id=EoMa_Chaos_Wyvern, race=monster, description={NO_DESCR_AVAILABLE}
#: Era_of_Magic/units/Dark_Blood_Alliance/Chaos_Wyvern.cfg:4
msgid "Chaos Wyvern"
msgstr "Guiverno del caos"

#. [attack]: type=cold
#: Era_of_Magic/units/Dark_Blood_Alliance/Chaos_Wyvern.cfg:56
#: Era_of_Magic/units/Dark_Blood_Alliance/Chaos_Wyvern_Rider.cfg:71
#: Era_of_Magic/units/Dark_Blood_Alliance/Great_Wyvern.cfg:55
#: Era_of_Magic/units/Destroyers/Devourer.cfg:67
